{
    "name": "pkboom/laravel-tinker-on-vscode",
    "description": "Tinker on vscode",
    "keywords": [
        "pkboom",
        "tinker",
        "vscode"
    ],
    "homepage": "https://github.com/pkboom/laravel-tinker-on-vscode",
    "license": "MIT",
    "authors": [
        {
            "name": "Keunbae Park",
            "email": "ppotpo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/support": "^12.0",
        "laravel/tinker": "^2.10.1",
        "pkboom/file-watcher": "^0.2.6",
        "react/event-loop": "^1.2",
        "symfony/console": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\TinkerOnVscode\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkboom\\TinkerOnVscode\\TinkerOnVscodeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
